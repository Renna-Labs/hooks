{
    "name": "@rennalabs/hooks",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "typings": "types/index.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "scripts": {
        "start": "NODE_ENV=development babel -w ./src --out-dir ./dist --extensions \".ts,.tsx\"",
        "build": "NODE_ENV=production npm run clean && babel ./src --out-dir ./dist --extensions \".ts,.tsx\" && tsc",
        "clean": "rimraf types dist",
        "format": "prettier --write \"src/*.{ts,tsx,json,md}\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "prepare": "husky install",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write",
            "git add ."
        ],
        "*.{ts,tsx}": [
            "eslint --fix",
            "git add ."
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Renna-Labs/hooks.git"
    },
    "keywords": [
        "react",
        "library",
        "frontend",
        "react-hooks",
        "hooks",
        "state"
    ],
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "author": {
        "name": "Renna-Labs",
        "email": "tech@rennalabs.xyz",
        "twitter": "@rennalabs"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Renna-Labs/hooks/issues"
    },
    "homepage": "https://github.com/Renna-Labs/hooks#readme",
    "jest": {
        "testEnvironment": "jest-environment-jsdom",
        "testRegex": "test.js$",
        "moduleDirectories": [
            "./node_modules",
            "./src"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
        }
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@rennalabs/eslint-config": "^1.0.2",
        "@rennalabs/prettier-config": "^1.0.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.4.0",
        "@types/react": "^18.0.27",
        "@typescript-eslint/eslint-plugin": "^5.49.0",
        "@typescript-eslint/parser": "^5.49.0",
        "husky": "^8.0.3",
        "jest": "^29.4.1",
        "jest-environment-jsdom": "^29.4.1",
        "lint-staged": "^13.1.0",
        "prettier": "^2.8.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^4.1.2",
        "ts-jest": "^29.0.5",
        "typescript": "^4.9.5"
    }
}
